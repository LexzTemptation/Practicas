CREATE DATABASE clinicaDentista;

USE clinicaDentista;

CREATE TABLE pacientes
(
    id INT PRIMARY KEY,
    mombre VARCHAR(30),
    apellido_paterno VARCHAR(20),
    apellido_materno VARCHAR(20),
    edad INT,
    direccion VARCHAR(100),
    telefono INT
);

CREATE TABLE citas
(
    fecha DATE PRIMARY KEY,
    hora DATE
);
